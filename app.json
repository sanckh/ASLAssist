{
  "expo": {
    "name": "aslapp",
    "slug": "aslapp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sanckh.aslassist",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.sanckh.aslassist",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
    	[
    		"onesignal-expo-plugin",
    		{
          "mode": "development"
          
        }
    	]
    ],
    "extra": {
      "oneSignalAppId" : "566f7f24-eca7-4c7e-bf1d-c3b96da22eac"
    }
  }
}
